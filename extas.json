{
  "name": "extas/protocols",
  "installer_version": 3,
  "package_classes": [
    {
      "interface": "protocolRepository",
      "class": "extas\\components\\protocols\\ProtocolRepository"
    }
  ],
  "plugins": [
    {
      "class": "extas\\components\\plugins\\init\\InitProtocols",
      "stage": "extas.init.section.protocols"
    },
    {
      "class": "extas\\components\\plugins\\uninstall\\UninstallProtocols",
      "stage": "extas.uninstall.section.protocols"
    }
  ],
  "extensions": [
    {
      "class": "extas\\components\\extensions\\ExtensionRepositoryGet",
      "interface": "extas\\interfaces\\extensions\\IExtensionRepositoryGet",
      "subject": "*",
      "methods": ["protocolRepository"]
    }
  ]
}